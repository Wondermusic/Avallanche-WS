<?xml version="1.0" encoding="utf-8"?>
<Window xmlns:x="http://www.w3.org/2001/XMLSchema-instance" x:type="PlayerWindow">
  <IsDefault>false</IsDefault>
  <Width>1280</Width>
  <Height>720</Height>

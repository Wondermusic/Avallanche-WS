          <Width>730</Width>
          <Height>84</Height>
          <PosX>470</PosX>
          <PosY>476</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="Avalon 74 White" />
